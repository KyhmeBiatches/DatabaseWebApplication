@model IEnumerable<DatabaseWebApplication.Answer>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PrefabAnswers.text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Question.text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Survey.title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SurveyUser.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bool_a)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.answer1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PrefabAnswers.text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Question.text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Survey.title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SurveyUser.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bool_a)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.answer1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
